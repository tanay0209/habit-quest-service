model User {
    id           String     @id @default(uuid())
    username     String     @unique
    email        String     @unique
    password     String?
    googleId     String?    @unique
    refreshToken String?
    habits       Habit[]
    categories   Category[]
    coins        Int        @default(100)
    createdAt    DateTime   @default(now())
    maxHabit     Int        @default(5)
    habitCount   Int        @default(0)
    updatedAt    DateTime   @updatedAt

    @@index([username])
    @@index([email])
}
